{
	"name": "friendsoftypo3headless/linkitems",
	"type": "typo3-cms-extension",
	"description": "Adds a configurable linkitems elements as one-to-many relation to easliy add links or buttons to any content element",
	"license": [
		"MIT"
	],
	"keywords": [
		"TYPO3 CMS",
		"linkitems",
		"links"
	],
	"require": {
		"typo3/cms-core": "^10.4 || ^11.5",
		"typo3/cms-extbase": "^10.4 || ^11.5"
	},
	"require-dev": {
		"symplify/easy-coding-standard": "^9.4",
		"phpmd/phpmd": "^2.9",
		"typo3/cms-lowlevel": "^11.5",
		"squizlabs/php_codesniffer": "^3.5",
		"helmich/typo3-typoscript-lint": "^2.5",
		"phpstan/phpstan": "^0.12.91",
		"thecodingmachine/phpstan-strict-rules": "^0.12.1",
		"friendsoftypo3/phpstan-typo3": "^0.8.1",
		"phpstan/phpstan-deprecation-rules": "^0.12.6",
		"slam/phpstan-extensions": "^5.0",
		"saschaegerer/phpstan-typo3": "^0.13.3",
		"phpstan/extension-installer": "^1.1"
	},
	"autoload": {
		"psr-4": {
			"FriendsOfTypo3Headless\\Linkitems\\": "Classes/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "linkitems"
		}
	},
	"config": {
		"allow-plugins": {
			"composer/package-versions-deprecated": true,
			"typo3/cms-composer-installers": true,
			"typo3/class-alias-loader": true,
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"phpstan/extension-installer": true
		}
	}
}
